<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OutlineConstraintLayout">
        <attr name="outlineRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArrowItemView">
        <attr name="titleText" format="string" />
        <attr name="colorTitleText" format="color" />
        <attr name="sizeTitleText" format="dimension" />
        <attr name="fontTitleText" format="reference" />
        <attr name="icon" format="reference" />
        <attr name="isArrowVisible" format="boolean" />
        <attr name="arrowTint" format="color" />
    </declare-styleable>

    <declare-styleable name="ShadowView">
        <attr name="shadowDrawable" format="reference" />>
    </declare-styleable>

    <declare-styleable name="FlowLayoutManager">
        <attr name="spanCount" format="integer" />>
    </declare-styleable>
</resources>
